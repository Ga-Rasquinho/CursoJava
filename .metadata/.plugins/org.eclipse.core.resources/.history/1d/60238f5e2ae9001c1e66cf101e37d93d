package application;



public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 0; int j = 0;

		if (i++ == ++j) {

		System.out.println("True: i=" + i + ", j=" + j); 

		} else {

		System.out.println("False: i=" + i + ", j=" + j); 

		} 
	}

}
