package entities;

public class GerenciamentoPedidos extends Empregado {
	private int idPedido;

	public GerenciamentoPedidos(int idUsuario, String nome, double salario, int tipoUsuario, String funcaoEmpregado,
			int idPedido) {
		super(idUsuario, nome, salario, tipoUsuario, funcaoEmpregado);
		this.idPedido = idPedido;
	}
	
	
}
