package application;



public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean isValid = true;
		while (isValid){
		isValid = false;
		System.out.print("test1 ");
		if (isValid = true) {
		System.out.println("test2 ");
		break;
		}
		isValid = false;
		System.out.println("test3");
		}
	}

}
